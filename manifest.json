{
  "manifest_version": 2,
  "name": "ycbm",
  "version": "0.1.0",
  "description": "Yet-Another Chrome Bookmark Manager",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  //"default_locale": "ja",
  "permissions": [
    "bookmarks"
  ],

  "background": {
    "page": "background.html"
  },
  "background": {
    "scripts": [
       "js/background.js"
     ],
     "persistent": false
  },
  
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Yet-Another Chrome Bookmark Manager"
  },
  "options_page": "options.html",
  "permissions": [
     "bookmarks"
  ]
}
